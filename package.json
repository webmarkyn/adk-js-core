{
  "name": "@google/adk",
  "version": "0.1.0-rc.0",
  "description": "Google ADK JS",
  "author": "Google",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/google/adk-js"
  },
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "dist"
  ],
  "type": "module",
  "typings": "./dist/types/index.d.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/web/index_web.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "clean:all": "npm run clean && rm -rf node_modules",
    "build": "tsc --emitDeclarationOnly && node ./build.js",
    "build:bundle": "tsc --emitDeclarationOnly && node ./build.js --bundle",
    "build:watch": "concurrently \"tsc --noEmit --watch --preserveWatchOutput\" \"node ./build.js --watch\"",
    "test": "vitest",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts'",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "concurrently": "^9.2.1",
    "esbuild": "^0.25.9",
    "eslint-config-prettier": "^9.1.0",
    "eslint": "^8.57.0",
    "openapi-types": "^12.1.3",
    "prettier": "^3.2.5",
    "tslib": "^2.6.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@google/genai": "1.14.0",
    "@modelcontextprotocol/sdk": "1.17.5",
    "@opentelemetry/api": "1.9.0",
    "zod": "3.25.76"
  }
}
