{
  "name": "@google/adk",
  "version": "0.2.2",
  "description": "Google ADK JS",
  "author": "Google",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/google/adk-js"
  },
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "dist"
  ],
  "type": "module",
  "typings": "./dist/types/index.d.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/web/index_web.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "clean:all": "npm run clean && rm -rf node_modules",
    "build": "tsc --emitDeclarationOnly && node ./build.js",
    "build:bundle": "tsc --emitDeclarationOnly && node ./build.js --bundle",
    "build:watch": "concurrently \"tsc --noEmit --watch --preserveWatchOutput\" \"node ./build.js --watch\"",
    "test": "vitest",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts'",
    "prepublishOnly": "npm run build:bundle"
  },
  "dependencies": {
    "@google/genai": "1.32.0",
    "@modelcontextprotocol/sdk": "^1.24.0",
    "google-auth-library": "^10.3.0",
    "lodash-es": "^4.17.22",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "openapi-types": "^12.1.3"
  },
  "peerDependencies": {
    "@google-cloud/opentelemetry-cloud-monitoring-exporter": "^0.21.0",
    "@google-cloud/opentelemetry-cloud-trace-exporter": "^3.0.0",
    "@google-cloud/storage": "^7.17.1",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/api-logs": "^0.205.0",
    "@opentelemetry/exporter-logs-otlp-http": "^0.205.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.205.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.205.0",
    "@opentelemetry/resource-detector-gcp": "^0.40.0",
    "@opentelemetry/resources": "^2.1.0",
    "@opentelemetry/sdk-logs": "^0.205.0",
    "@opentelemetry/sdk-metrics": "^2.1.0",
    "@opentelemetry/sdk-trace-base": "^2.1.0",
    "@opentelemetry/sdk-trace-node": "^2.1.0"
  }
}
